    <xs:import namespace="https://eml.ecoinformatics.org/attribute-2.2.0" schemaLocation="eml-attribute.xsd"/>
    <xs:import namespace="https://eml.ecoinformatics.org/constraint-2.2.0" schemaLocation="eml-constraint.xsd"/>
